<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.tripon.reviewpicture.domain.ReviewPictureRepository">

    <insert id="save" parameterType="ReviewPicture" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO pictures (reviewdetail_id, picture_original, picture_modify)
        VALUES (#{reviewDetailId}, #{originalName}, #{url})
    </insert>

</mapper>