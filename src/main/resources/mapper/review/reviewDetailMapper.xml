<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.tripon.review.domain.ReviewDetailRepository">

    <insert id="save" parameterType="ReviewDetail" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO reviewdetails (review_id, day, content)
        VALUES (#{reviewId}, #{day}, #{content})
    </insert>

</mapper>